		"eslint-plugin-svelte3": "^4.0.0",
		"install": "^0.13.0",
		"npm": "^8.15.1",
		"or": "^0.2.0",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.0",
